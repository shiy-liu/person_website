{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:vue/essential"
  ],
  "rules": {
    "no-console": 0,
    "no-duplicate-case": 2,
    "no-empty-function": 2,
    "no-empty": 2,
    "no-dupe-args": 2,
    "camelcase": 0,
    "max-depth": 2,
    "no-debugger": 2,
    "no-proto": 2,
    "no-label-var": 2,
    "no-mixed-spaces-and-tabs": 2,
    "indent": ["error", 2],
    "require-yield": 2,
    "prefer-arrow-callback": 2,
    "no-var": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "max-nested-callbacks": 2,
    "no-undef-init": 2,
    "no-delete-var": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-else-return": 2,
    "no-unreachable": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    //禁止使用稀疏数组
    "no-sparse-arrays": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-multiple-empty-lines": [2, { "max": 2 } ], //空行不能够超过2行
    "no-inner-declarations": [2, "functions"],
    "no-const-assign": 2
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}